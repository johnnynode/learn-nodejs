<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <form action="/add" method='post'>
      <label>
        <input type="text" name='title'> 标题
      </label>
      <input type="submit" value="添加">
    </form>
    <ul>
      <% list.forEach(function(one, i) { %>
        <li><%= one.title %> <a href="/del?i=<%=i%>">删除</a></li>
      <% }) %>
    </ul>
  </body>
</html>
